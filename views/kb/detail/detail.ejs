<div class="content-wrapper">
<div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1 class="m-0">Detail</h1>
            </div><!-- /.col -->
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item">Home</li>
                    <li class="breadcrumb-item">Kriteria</li>
                    <li class="breadcrumb-item">Detail</li>
                </ol>
            </div><!-- /.col -->
        </div><!-- /.row -->
    </div><!-- /.container-fluid -->
</div>

    <div class="container">
        <div class="row ">
            <div class="col-lg-8 offset-lg-2">
                <h2 class="font-weight-bold text-center mb-4">Detail dari Kriteria
                </h2>
            </div>
        </div>
        <div class="row ">
            <% if(data){%>
                <% data.forEach(function(detail,index) { %>
                    <div class="col-lg-4 col-sm-12 d-flex justify-content-center">
                        <div class="box4 shadow p-3 mb-5 bg-white rounded">
                            <div class="row">
                                <div class="col-lg-3">
                                    <div class="box5"></div>
                                </div>
                                <div class="col-lg-9">
                                    <p style="font-size: 16px;" class="font-weight-bold">
                                        <%=detail.nm_alternatif %>
                                    </p>
                                    <p style="text-align:justify; font-size: 12px;">
                                        <%= detail.keterangan %>
                                    </p>
                                </div>
                            </div>


                        </div>
                    </div>
                    <% }) %>
                        <%}%>
        </div>
        <div class="row ">
            <div class="col-lg-12 m-auto text-center">
                <a href="/criteria/<%= jenis %>">
                    <button type="submit" class="btn btn-primary mb-4 mt-4 " style="width:100%; ">
                        Kembali ke Kriteria</button></a>
            </div>
        </div>
    </div>
</div>